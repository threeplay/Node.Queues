{
  "name": "node.typescript.template",
  "version": "0.0.1",
  "description": "Base node typescript template",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "cover": "nyc mocha --opts ./mocha.opts",
    "test": "mocha --opts ./mocha.opts",
    "test-features": "cucumber-js -p default"
  },
  "nyc": {
    "extensions": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": "https://github.com/threeplay/Node.Queues",
  "author": "Eliran Ben Ezra",
  "license": "MIT",
  "engines": {
    "node": "^10.6.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@types/cucumber": "^4.0.7",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
  }
}
